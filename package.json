    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.5.12",
    "morgan": "^1.9.1",
    "react-transition-group": "^4.1.0",
    "reactstrap": "^8.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  },
  "engines": {
    "node": "v12.3.1"
  }
